<style>
.qr-code-image-centered {
    margin: auto;
}

.qr-code-image-wrapper-large {
    max-width: 270px;
}

.feid-qr-code-image-wrapper {
    position: relative;
}
    .feid-qr-code-image-wrapper .feid-qr-code-image {
        display: block;
        width: 100%;
        height: auto;
    }

    .feid-qr-code-image-disable {
        opacity: 0.3;
    }
    .feid-qr-code-image-wrapper .feid-qr-code-image-overlay {
      /*  position: absolute;*/
        top: -10px;
        left: -10px;
        height: 100%;
        width: 100%;
        background-color: #ffffff90;
        -webkit-border-radius: 8px;
        -moz-border-radius: 8px;
        border-radius: 8px;
        padding: 10px;
        -webkit-box-shadow: 0 4px 8px 0 rgb(35 31 32 / 20%);
        -moz-box-shadow: 0 4px 8px 0 rgba(35,31,32,0.2);
        box-shadow: 0 4px 8px 0 rgb(35 31 32 / 20%);
    }
        .feid-qr-code-image-wrapper .feid-qr-code-image-overlay .feid-qr-code-reload-icon {
            opacity: 1;
            background-color: #f7f6f7;
            -webkit-border-radius: 100%;
            -moz-border-radius: 100%;
            border-radius: 100%;
            position: absolute;
            top: 50%;
            left: 50%;
            -webkit-transform: translate(-50%, -50%);
            -moz-transform: translate(-50%, -50%);
            transform: translate(-50%, -50%);
        }

    .qr-code-reload-icon-large {
        height: 120px;
        width: 120px;
    }
    .feid-qr-code-reload-text {
        max-width: 270px;
        position: absolute;
        bottom: 20px;
        left: 50%;
        -webkit-transform: translate(-50%, 0);
        -moz-transform: translate(-50%, 0);
        transform: translate(-50%, 0);
    }
        .feid-qr-code-reload-text p {
            margin: 0;
        }
    p.feid-text, span.feid-text {
        font-size: calc( 13px + 2 * ((100vw - 320px) / 880));
    }

    p.feid-text, span.feid-text {
        font-size: 13px;
    }

    p.feid-text, span.feid-text {
        font-family: "Montserrat",sans-serif;
        line-height: 1.3;
    }
</style>
