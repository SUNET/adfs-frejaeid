<script type="text/javascript">
    $(document).ready(function () {

  $("#btngetemp").click(function () {
      var url = $(this).attr("href");
      $.ajax({
          url: url,
          success: function (result) {
              debugger;
              $("#emimage").attr("src", result);
          },
      });


});
});
</script>